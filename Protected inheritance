#include<iostream>
#include<string>
using namespace std;

  class Person{
      protected:
      string name;
      
      public:
      void setname(string iname){name=iname;}
     
      
      
      
  };
class Student: public Person{
public:
 void display(){
     
     cout<<name<<endl;
 }
};


int main(){
    
    Student obj;
    obj.setname("dharya");
    obj.display();
    
    
    return 0;
}
