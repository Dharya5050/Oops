
#include <iostream>
#include <string>
using namespace std;

class Person{
    public:
     void introduce(){
         cout<<"Hey I am Dharya"<<endl;
         }
    
    
    
};

class Student:public Person{
    public:
    void introduce(){
    cout<<"Hey I am akshit"<<endl;
    }
    
    
    
    
};

void whosthis(Person p){
    p.introduce();
    
    
}
    
    

int main()
{ Student obj;
  obj.introduce();
  whosthis(obj);
  
  
    
    return 0;
}
